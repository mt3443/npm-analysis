// interface String {
//     isNullOrEmpty(item: string): boolean;
// }
// String.prototype.isNullOrEmpty = function(item: string): boolean {
//     return !item || 0 === item.length;
// }
var StringExtensions = /** @class */ (function () {
    function StringExtensions() {
    }
    StringExtensions.isNullOrEmpty = function (item) {
        return !item || 0 === item.length;
    };
    return StringExtensions;
}());
export { StringExtensions };
