var ColumnGroup = /** @class */ (function () {
    function ColumnGroup(Name, Header, HeaderTextAlign) {
        this.Name = Name;
        this.Header = Header;
        this.HeaderTextAlign = HeaderTextAlign;
        this.HeaderTextAlign = HeaderTextAlign || this.DefaultHeaderTextAlign;
    }
    Object.defineProperty(ColumnGroup.prototype, "DefaultHeaderTextAlign", {
        get: function () {
            return "center";
        },
        enumerable: true,
        configurable: true
    });
    return ColumnGroup;
}());
export { ColumnGroup };
