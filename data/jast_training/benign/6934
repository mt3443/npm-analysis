var Validation = /** @class */ (function () {
    function Validation() {
    }
    Validation.prototype.Set = function (message) {
        this.Error = message;
        this.HasValidation = true;
    };
    Validation.prototype.Clear = function () {
        this.Error = "";
        this.HasValidation = false;
    };
    return Validation;
}());
export { Validation };
